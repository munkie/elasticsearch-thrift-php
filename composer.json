{
    "name": "munkie/elasticsearch-thrift-php",
    "description": "ElasticSearch PHP Thrift transport client. Generated from elasticsearch-transport-thrift plugin thrift file",
    "authors": [
        {
            "name": "Mikhail Shamin",
            "email": "munk13@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": ["thrift", "elasticsearch"],
    "autoload": {
        "classmap": ["src/Elasticsearch"]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "elasticsearch/elasticsearch-transport-thrift",
                "version": "1.4.2",
                "dist": {
                    "url": "https://raw.github.com/elasticsearch/elasticsearch-transport-thrift/a7c45d4d7ef4cec4a094791f8639a54872243464/elasticsearch.thrift",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "leric/php-thrift": "0.9.0"
    },
    "require-dev": {
        "elasticsearch/elasticsearch-transport-thrift": "1.4.*"
    }
}
